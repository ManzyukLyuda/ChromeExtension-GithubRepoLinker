{
    "name": "Github repo linker",
    "description": "Easy way to find GitHub project with Github Pages link",
    "version": "0.1",

    "permissions": ["https://*.prototype-land.firebaseapp.com/*", "contextMenus", "notifications"],
    "background": {
        "scripts": ["background.js"]
